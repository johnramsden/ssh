# ssh configuration file
# John Ramsden

######### Hosts ########

## External Hosts ##

Host *.skvare.com
  ForwardAgent yes
  port 27271
  ControlMaster no
  ControlPath none

# Fallback:
# Matches anything that doesn't have a value at this point
Host *
  User john
  ControlMaster auto
  ControlPersist yes
  ControlPath ~/.ssh/sockets/socket-%r@%h:%p
  IdentityFile ~/.ssh/id_ed25519
  AddKeysToAgent yes
